   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "with open(\"3_data.txt\") as f:\n",
    "    data = f.read()\n",
    "    \n",
    "data = data.split('\\n')\n",
    "\n",
    "x = [row.split(' ')[0] for row in data]\n",
    "y = [row.split(' ')[1] for row in data]\n",
    "plt.plot(x, y)\n",
    "\n",
    "plt.xlabel('x axis')\n",
    "plt.ylabel('y axis')\n",
    "plt.title('graph')\n",
    "plt.show()"
   ]
  }
